@model ChangeRoleVM
@{
    ViewData["Title"] = "ChangeRole";
    IEnumerable<RoleVM> roleVMs = ViewBag.Roles;
    SelectList selectListItems = new SelectList(roleVMs, nameof(RoleVM.Id), nameof(RoleVM.Name));
}
<div class="row">
    <div class="col-lg-12">
        <div class="row justify-content-between">
            <div class="col-lg-3">
                <h1 class="h3 mb-4 text-gray-800">Role Change Page</h1>
            </div>

        </div>
    </div>
    <div class="col-lg-12 ">
        <form method="post" enctype="multipart/form-data">
            @*<input type="hidden" name="UserId" value="@Model.UserId"/>*@
            <div class="form-group parentInput">
                <label asp-for="RoleId"></label>
                <select class="form-control" asp-for="RoleId" asp-items="@selectListItems">
                </select>
                <span asp-validation-for="RoleId" class="text-danger"></span>

            </div>

            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>
</div>